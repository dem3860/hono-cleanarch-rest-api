generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event { 
  id   String   @id @default(uuid())
  name String
  description String
  startDate DateTime
  endDate DateTime
  location String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}